{
  "buildCommand": "npm run build",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "outputDirectory": "dist/public",
  "functions": {
    "dist/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NERO_AI_API_KEY": "4CI5GNA0UD2UWFDZK5MPCUQ1"
  }
}